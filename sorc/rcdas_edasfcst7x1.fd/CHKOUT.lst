   2247       SUBROUTINE COAL(A,LEN)
   2248       INCLUDE "BUFFER.comm"
   2257       INCLUDE 'mpif.h'
   2819       REAL A(*)
   2820       IF ( LEN .LT. 0 ) THEN
   2821          IP = 0
   2822       END IF
   2823       IF ( IP + LEN .GT. IBUFMAX ) THEN
   2824          PRINT *, ' IBUFMAX in BUFFER.comm is too small, stopping'
   2825          PRINT *, ' CHANGE IBUFMAX in parmbuf and recompile'       
   2826          PRINT *, ' IBUFMAX=', IBUFMAX, ' WANT AT LEAST ', IP + LEN
   2827          CALL MPI_ABORT(MPI_COMM_WORLD,1,IERR)
   2828       ENDIF
   2829       DO I = 1, ABS(LEN)
   2830          IP = IP + 1
   2831          BUF(IP) = A(I)
   2832       ENDDO
   2833       return
   2834       END


