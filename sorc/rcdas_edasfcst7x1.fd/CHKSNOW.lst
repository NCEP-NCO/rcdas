      1 # 1 "CHKSNOW.F"
      2       subroutine chksnow
      3 c
      4 c At the first physics time step after the top of each hour, check the snow
      5 c array against the SR (snow/total precip ratio).  If SR .ge. 0.9, set this
      6 c point to missing (so we won't do snow adjustment here).
      7 c
      8       INCLUDE "parmeta"
     31       INCLUDE "mpif.h"
    593       INCLUDE "mpp.h"
    652                              P A R A M E T E R
    653      & (ITB=76,JTB=134,ITBQ=152,JTBQ=440,LP1=LM+1)
    654 C-----------------------------------------------------------------------
    655       INCLUDE "PPTASM.comm"
    656 C-----------------------------------------------------------------------
    680       INCLUDE "CLDWTR.comm"
    681 C-----------------------------------------------------------------------
    689       INCLUDE "CTLBLK.comm"
    690 C-----------------------------------------------------------------------
    700       INCLUDE "PHYS2.comm"
    701 C-----------------------------------------------------------------------
    729                              P A R A M E T E R
    730      & (RTD=57.2957795)
    731 C
    732       TIMES=(NTSD-1)*DT
    733       if (mod(times,3600.) .lt. nphs*dt) then
    734         ihr=(times-1.0)/3600.+1
    735         call loc2glb(sr,temp1)
    736         if (mype.eq.0) then 
    737            write(98,*) 'NTSD=', NTSD,' times=', times,' output SR:'
    738            write(90) temp1
    739         endif
    740         DO 20 J=MYJS,MYJE
    741           DO 10 I=MYIS,MYIE
    742 C
    743 C In the operational Eta, we do not use the data in the area where ETA
    744 C thinks it's snowing (we use sr(i,j).ge.0.9 as criterion), because the
    745 C real-time hourly observations (radar or gauge) are highly unreliable.
    746 C For reanalysis, more reliable hourly snow observations may be available
    747 C within CONUS, so we are changing it to 'mask out the precip data when
    748 C it's north of 49N and likely to be snowing'.
    749 C
    750 C OPNL      if (sr(i,j).ge.0.9) pptdat(i,j,ihr) = 999.
    751             if (glat(i,j)*RTD.ge.49. .and. sr(i,j).ge.0.9) 
    752      &         pptdat(i,j,ihr) = 999.
    753   10	  continue
    754   20    continue
    755       endif
    756       return
    757       end

