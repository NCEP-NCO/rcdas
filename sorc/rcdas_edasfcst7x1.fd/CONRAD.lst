      2                     SUBROUTINE CONRAD(NFILE)
      3 C
      4 C-----------------------------------------------------------------------
      5       INCLUDE "parmeta"
     28       INCLUDE "mpif.h"
    590       INCLUDE "mpp.h"
    591 # 1 "./sp.h" 1 
    592 # 4
    593 
    594 # 8 "CONRAD.F" 2 
    595 C-----------------------------------------------------------------------
    654                         P A R A M E T E R
    655      1 (IX=2*IM-1,KX=LM)
    656 C    *******************************************************************
    657 C    *                           C O N R A D                           *
    658 C    *    READ CO2 TRANSMISSION DATA FROM UNIT(NFILE)FOR NEW VERTICAL  *
    659 C    *      COORDINATE TESTS      ...                                  *
    660 C    *    THESE ARRAYS USED TO BE IN BLOCK DATA    ...K.CAMPANA-MAR 90 *
    661 C    *******************************************************************
    662 C
    663       PARAMETER (L=KX,LP1=KX+1,KP=KX+1,LP12=LP1*LP1)
    664       DIMENSION SGTMP(LP1,2),CO21D(L,6),CO22D(LP1,LP1,6)
    665       DIMENSION CO21D3(LP1,6),CO21D7(LP1,6)
    666       DIMENSION RSZE(3),DATA2(LP12)
    667       INTEGER RSZE,RSIZE
    668 C
    669 C                 CO2 DATA TABLES FOR USER''S VERTICAL COORDINATE
    670 C
    671 C   THE FOLLOWING COMMON BLOCKS CONTAIN PRETABULATED CO2 TRANSMISSION
    672 C       FUNCTIONS, EVALUATED USING THE METHODS OF FELS AND
    673 C       SCHWARZKOPF (1981) AND SCHWARZKOPF AND FELS (1985),
    674 C-----  THE 2-DIMENSIONAL ARRAYS ARE
    675 C                    CO2 TRANSMISSION FUNCTIONS AND THEIR DERIVATIVES
    676 C        FROM 109-LEVEL LINE-BY-LINE CALCULATIONS MADE USING THE 1982
    677 C        MCCLATCHY TAPE (12511 LINES),CONSOLIDATED,INTERPOLATED
    678 C        TO THE NMC MRF VERTICAL COORDINATTE,AND RE-CONSOLIDATED TO A
    679 C        200 CM-1 BANDWIDTH. THE INTERPOLATION METHOD IS DESCRIBED IN
    680 C        SCHWARZKOPF AND FELS (J.G.R.,1985).
    681 C-----  THE 1-DIM ARRAYS ARE
    682 C                  CO2 TRANSMISSION FUNCTIONS AND THEIR DERIVATIVES
    683 C          FOR TAU(I,I+1),I=1,L,
    684 C            WHERE THE VALUES ARE NOT OBTAINED BY QUADRATURE,BUT ARE THE
    685 C            ACTUAL TRANSMISSIVITIES,ETC,BETWEEN A PAIR OF PRESSURES.
    686 C          THESE USED ONLY FOR NEARBY LAYER CALCULATIONS INCLUDING QH2O.
    687 C-----  THE WEIGHTING FUNCTION GTEMP=P(K)**0.2*(1.+P(K)/30000.)**0.8/
    688 C         1013250.,WHERE P(K)=PRESSURE,NMC MRF(NEW)  L18 DATA LEVELS FOR
    689 C         PSTAR=1013250.
    690 C-----  STEMP IS US STANDARD ATMOSPHERES,1976,AT DATA PRESSURE LEVELS
    691 C        USING NMC MRF SIGMAS,WHERE PSTAR=1013.25 MB (PTZ PROGRAM)
    692 C***COMMON CO2BD3 CONTAINS CO2 TRANSMISSION FUNCTIONS AND TEMPERATURE
    693 C   AND PRESSURE DERIVATIVES FOR THE 560-800 CM-1 BAND. ALSO INCLUDED
    694 C   ARE THE STANDARD TEMPERATURES AND THE WEIGHTING FUNCTION. THESE
    695 C   DATA ARE IN BLOCK DATA BD3:
    696 C         CO251    =  TRANSMISSION FCTNS FOR T0 (STD. PROFILE)
    697 C                       WITH P(SFC)=1013.25 MB
    698 C         CO258    =  TRANSMISSION FCTNS. FOR T0 (STD. PROFILE)

Page 2           Source Listing                  CONRAD
2022-02-15 16:36                                 CONRAD.F

    699 C                       WITH P(SFC)= 810 MB
    700 C         CDT51    =  FIRST TEMPERATURE DERIVATIVE OF CO251
    701 C         CDT58    =  FIRST TEMPERATURE DERIVATIVE OF CO258
    702 C         C2D51    =  SECOND TEMPERATURE DERIVATIVE OF CO251
    703 C         C2D58    =  SECOND TEMPERATURE DERIVATIVE OF CO251
    704 C         CO2M51   =  TRANSMISSION FCTNS FOR T0 FOR ADJACENT PRESSURE
    705 C                        LEVELS, WITH NO PRESSURE QUADRATURE. USED FOR
    706 C                        NEARBY LAYER COMPUTATIONS. P(SFC)=1013.25 MB
    707 C         CO2M58   =  SAME AS CO2M51,WITH P(SFC)= 810 MB
    708 C         CDTM51   =  FIRST TEMPERATURE DERIVATIVE OF CO2M51
    709 C         CDTM58   =  FIRST TEMPERATURE DERIVATIVE OF CO2M58
    710 C         C2DM51   =  SECOND TEMPERATURE DERIVATIVE OF CO2M51
    711 C         C2DM58   =  SECOND TEMPERATURE DERIVATIVE OF CO2M58
    712 C         STEMP    =  STANDARD TEMPERATURES FOR MODEL PRESSURE LEVEL
    713 C                        STRUCTURE WITH P(SFC)=1013.25 MB
    714 C         GTEMP    =  WEIGHTING FUNCTION FOR MODEL PRESSURE LEVEL
    715 C                        STRUCTURE WITH P(SFC)=1013.25 MB.
    716 C-----       THE FOLLOWING ARE STILL IN BLOCK DATA
    717 C         B0       =  TEMP. COEFFICIENT USED FOR CO2 TRANS. FCTN.
    718 C                        CORRECTION FOR T(K). (SEE REF. 4 AND BD3)
    719 C         B1       =  TEMP. COEFFICIENT, USED ALONG WITH B0
    720 C         B2       =  TEMP. COEFFICIENT, USED ALONG WITH B0
    721 C         B3       =  TEMP. COEFFICIENT, USED ALONG WITH B0
    722 C
    723       COMMON/CO2BD3/CO251(LP1,LP1),CO258(LP1,LP1),CDT51(LP1,LP1),
    724      1 CDT58(LP1,LP1),C2D51(LP1,LP1),C2D58(LP1,LP1),CO2M51(L),
    725      2 CO2M58(L),CDTM51(L),CDTM58(L),C2DM51(L),C2DM58(L),
    726      3 STEMP(LP1),GTEMP(LP1),B0,B1,B2,B3
    727 C
    728 C***COMMON CO2BD2 CONTAINS CO2 TRANSMISSION FUNCTIONS AND TEMPERATURE
    729 C   AND PRESSURE DERIVATIVES FOR THE 560-670 CM-1 PART OF THE 15 UM
    730 C   CO2 BAND.  THESE DATA ARE IN BLOCK DATA BD2.
    731 C     FOR THE 560-670 CM-1 BAND,ONLY THE (1,I) VALUES ARE USED , SINCE
    732 C     THESE ARE USED FOR CTS COMPUTATIONS.
    733 C         CO231    =  TRANSMISSION FCTNS FOR T0 (STD. PROFILE)
    734 C                       WITH P(SFC)=1013.25 MB
    735 C         CO238    =  TRANSMISSION FCTNS. FOR T0 (STD. PROFILE)
    736 C                       WITH P(SFC)= 810 MB
    737 C         CDT31    =  FIRST TEMPERATURE DERIVATIVE OF CO231
    738 C         CDT38    =  FIRST TEMPERATURE DERIVATIVE OF CO238
    739 C         C2D31    =  SECOND TEMPERATURE DERIVATIVE OF CO231
    740 C         C2D38    =  SECOND TEMPERATURE DERIVATIVE OF CO231
    741 C
    742       COMMON / CO2BD2 / CO231(LP1),CO238(LP1),CDT31(LP1),
    743      1 CDT38(LP1),C2D31(LP1),C2D38(LP1)
    744 C
    745 C***COMMON CO2BD4 CONTAINS CO2 TRANSMISSION FUNCTIONS AND TEMPERATURE
    746 C   AND PRESSURE DERIVATIVES FOR THE 670-800 CM-1 PART OF THE 15 UM
    747 C   CO2 BAND.  THESE DATA ARE IN BLOCK DATA BD4.
    748 C         CO271    =  TRANSMISSION FCTNS FOR T0 (STD. PROFILE)
    749 C                       WITH P(SFC)=1013.25 MB
    750 C         CO278    =  TRANSMISSION FCTNS. FOR T0 (STD. PROFILE)
    751 C                       WITH P(SFC)= 810 MB
    752 C         CDT71    =  FIRST TEMPERATURE DERIVATIVE OF CO271
    753 C         CDT78    =  FIRST TEMPERATURE DERIVATIVE OF CO278
    754 C         C2D71    =  SECOND TEMPERATURE DERIVATIVE OF CO271
    755 C         C2D78    =  SECOND TEMPERATURE DERIVATIVE OF CO271

Page 3           Source Listing                  CONRAD
2022-02-15 16:36                                 CONRAD.F

    756 C
    757       COMMON / CO2BD4 / CO271(LP1),CO278(LP1),CDT71(LP1),
    758      1 CDT78(LP1),C2D71(LP1),C2D78(LP1)
    759 C
    760 C *****THE FOLLOWING NOT USED IN CURRENT VERSION OF RADIATION *******
    761 C
    762 C --COMMON CO2BD5 CONTAINS CO2 TRANSMISSION FUNCTIONS FOR THE 2270-
    763 C                                  2380 PART OF THE 4.3 UM CO2 BAND.
    764 C              THESE DATA ARE IN BLOCK DATA BD5.
    765 C         CO211    =  TRANSMISSION FCTNS FOR T0 (STD. PROFILE)
    766 C                        WITH P(SFC)=1013.25 MB
    767 C         CO218    =  TRANSMISSION FCTNS. FOR T0 (STD. PROFILE)
    768 C                       WITH P(SFC)= 810 MB
    769 C
    770       COMMON / CO2BD5 / CO211(LP1),CO218(LP1)
    771 C
    772 C *****THE ABOVE NOT USED IN CURRENT VERSION OF RADIATION ***********
    773 C
    774 C====>   BEGIN HERE TO GET CONSTANTS FOR RADIATION PACKAGE
    775       REWIND NFILE
    776 C       READ IN PRE-COMPUTED CO2 TRANSMISSION DATA....
    777 C....     AND CONVERT TO CYBER WORDS
    778       RSZE(1) = LP1
    779       RSZE(2) = L
    780       RSZE(3) = LP1*LP1
    781 C-----------------------------------------------------------------
    782       RSIZE = RSZE(1)
    783       DO 10 KK=1,2
    784       IF(MYPE.EQ.0)READ(NFILE)(SGTMP(I,KK),I=1,RSIZE)
    785       CALL MPI_BCAST(SGTMP(1,KK),RSIZE,MPI_REAL,0,
    786      1               MPI_COMM_COMP,IRTN)
    787    10 CONTINUE
    788 C
    789 C-----------------------------------------------------------------
    790       RSIZE = RSZE(2)
    791       DO 15 KK=1,6
    792       IF(MYPE.EQ.0)READ(NFILE)(CO21D(I,KK),I=1,RSIZE)
    793       CALL MPI_BCAST(CO21D(1,KK),RSIZE,MPI_REAL,0,
    794      1               MPI_COMM_COMP,IRTN)
    795    15 CONTINUE
    796 C
    797 C-----------------------------------------------------------------
    798       RSIZE = RSZE(3)
    799       DO 20 KK=1,6
    800       IF(MYPE.EQ.0)READ(NFILE)(DATA2(I),I=1,RSIZE)
    801       CALL MPI_BCAST(DATA2(1),RSIZE,MPI_REAL,0,
    802      1               MPI_COMM_COMP,IRTN)
    803 C
    804       N=0
    805       DO 5673 I1=1,LP1
    806       DO 5673 I2=1,LP1
    807       N=N+1
    808       CO22D(I1,I2,KK)=DATA2(N)
    809  5673 CONTINUE
    810    20 CONTINUE
    811 C
    812 C-----------------------------------------------------------------

Page 4           Source Listing                  CONRAD
2022-02-15 16:36                                 CONRAD.F

    813       RSIZE = RSZE(1)
    814       DO 25 KK=1,6
    815       IF(MYPE.EQ.0)READ(NFILE)(CO21D3(I,KK),I=1,RSIZE)
    816       CALL MPI_BCAST(CO21D3(1,KK),RSIZE,MPI_REAL,0,
    817      1               MPI_COMM_COMP,IRTN)
    818    25 CONTINUE
    819 C
    820 C-----------------------------------------------------------------
    821       DO 30 KK=1,6
    822       IF(MYPE.EQ.0)READ(NFILE)(CO21D7(I,KK),I=1,RSIZE)
    823       CALL MPI_BCAST(CO21D7(1,KK),RSIZE,MPI_REAL,0,
    824      1               MPI_COMM_COMP,IRTN)
    825    30 CONTINUE
    826 C
    827 C-----------------------------------------------------------------
    828       REWIND NFILE
    829       DO 35 K=1,LP1
    830         STEMP(K) = SGTMP(K,1)
    831         GTEMP(K) = SGTMP(K,2)
    832    35 CONTINUE
    833       DO 40 K=1,L
    834         CDTM51(K) = CO21D(K,1)
    835         CO2M51(K) = CO21D(K,2)
    836         C2DM51(K) = CO21D(K,3)
    837         CDTM58(K) = CO21D(K,4)
    838         CO2M58(K) = CO21D(K,5)
    839         C2DM58(K) = CO21D(K,6)
    840    40 CONTINUE
    841       DO 45 J=1,LP1
    842        DO 45 I=1,LP1
    843         CDT51(I,J) = CO22D(I,J,1)
    844         CO251(I,J) = CO22D(I,J,2)
    845         C2D51(I,J) = CO22D(I,J,3)
    846         CDT58(I,J) = CO22D(I,J,4)
    847         CO258(I,J) = CO22D(I,J,5)
    848         C2D58(I,J) = CO22D(I,J,6)
    849    45 CONTINUE
    850       DO 50 K=1,LP1
    851         CDT31(K) = CO21D3(K,1)
    852         CO231(K) = CO21D3(K,2)
    853         C2D31(K) = CO21D3(K,3)
    854         CDT38(K) = CO21D3(K,4)
    855         CO238(K) = CO21D3(K,5)
    856         C2D38(K) = CO21D3(K,6)
    857    50 CONTINUE
    858       DO 55 K=1,LP1
    859         CDT71(K) = CO21D7(K,1)
    860         CO271(K) = CO21D7(K,2)
    861         C2D71(K) = CO21D7(K,3)
    862         CDT78(K) = CO21D7(K,4)
    863         CO278(K) = CO21D7(K,5)
    864         C2D78(K) = CO21D7(K,6)
    865    55 CONTINUE
    866       IF(MYPE.EQ.0)PRINT 66,NFILE
    867    66 FORMAT(1H ,'----READ CO2 TRANSMISSION FUNCTIONS FROM UNIT ',I2)
    868 C
    869 C......    DEFINE TABLES FOR LW RADIATION

Page 5           Source Listing                  CONRAD
2022-02-15 16:36                                 CONRAD.F

    870       CALL TABLE
    871 C
    872       RETURN
    873       END


