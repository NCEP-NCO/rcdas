    664 C***ZHAO
    665         PRESS(I,LP1)=QS(IR)*10.0
    666         TEMP(I,LP1)=ABS(TSFC(IR))
    667 c       if(mype.eq.13.and.i.eq.40) then
    668 c       if(mype.eq.13) then
    669 c         print*,'ir,i,lp1=',ir,i,lp1
    670 c         print*,'tsfc(ir)=',tsfc(ir)
    671 c       endif
    672         COSZEN(I) = COSZRO(IR)
