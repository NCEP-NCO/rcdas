     80 C-----------------------------------------------------------------------
     81       Q2(LMHM)=(DTOZS*AKQS*Q2(LMHK)-RSQ2(LMH2)*CF+Q2(LMHM))
     82      &        /((AKQ(LMH2)+AKQS)*DTOZS-CR(LMH2)*CF+1.)
     83 C
     84           DO 120 IVI=1,LMH2
     85       L=LMHM-IVI
     86       Q2(L)=(-CR(L)*Q2(L+1)+RSQ2(L))/CM(L)
     87  120  CONTINUE
