    444           GDFNB(I) = GDFNB(I) + DFN(I,LP1)*DFNTOP(I,N)
    445 330     CONTINUE
    446 350   CONTINUE
    447       DO 360 K=1,LP1
    448       DO 360 I=MYIS,MYIE
    449         FSWL(I,K) = UFSWL(I,K)-DFSWL(I,K)
    450 360   CONTINUE
    451       DO 370 K=1,L
    452       DO 370 I=MYIS,MYIE
    453         HSWL(I,K)=RADCON*(FSWL(I,K+1)-FSWL(I,K))/DP(I,K)
    454 370   CONTINUE
