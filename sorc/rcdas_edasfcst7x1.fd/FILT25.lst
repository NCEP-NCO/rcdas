    140 C
    141       DATA CF1/0.279372/,CF2/0.171943/,CF3/-0.006918/
    142      1,    CF4/0.077458/,CF5/-0.024693/,CF6/-0.012940/
    143 C----------------------------------------------------------------
    144 C
    145       IPC=IPASS                                                       
    146 C
    147       DO J=JDIM1,JDIM2
    148       DO I=IDIM1,IDIM2
    149         Z(I,J)=0.
    150       ENDDO
    151       ENDDO
    152 C***
    153 C***  FILTER THE INTERIOR POINTS WITH 25-PT BLECK FILTER
    154 C***
    155       DO 105 IP=1,IPC                                                 

