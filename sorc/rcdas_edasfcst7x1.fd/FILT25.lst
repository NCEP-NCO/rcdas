    214      o       +ZI(IIHW-1,J+1)*TM(IIHW-1,J+1)+ZI(IIHW,J+3)*TM(IIHW,J+3))
    215      1  +CF6*(ZI(I,J-4)*TM(I,J-4)+ZI(I+2,J)*TM(I+2,J)
    216      2       +ZI(I-2,J)*TM(I-2,J)+ZI(I,J+4)*TM(I,J+4)))
    217      3  *RSUMH
    218       ENDIF
    219 C
    220   101 CONTINUE                                                    
    221   102 CONTINUE                                                       
