    206      2       +ZI(IIHE,J+1)*TM(IIHE,J+1)+ZI(IIHW,J+1)*TM(IIHW,J+1))
    207      3  +CF3*(ZI(I-1,J-2)*TM(I-1,J-2)+ZI(I+1,J-2)*TM(I+1,J-2)
    208      4       +ZI(I+1,J+2)*TM(I+1,J+2)+ZI(I-1,J+2)*TM(I-1,J+2))
    209      5  +CF4*(ZI(I,J-2)*TM(I,J-2)+ZI(I,J+2)*TM(I,J+2)
    210      6       +ZI(I+1,J)*TM(I+1,J)+ZI(I-1,J)*TM(I-1,J))
    211      7  +CF5*(ZI(IIHE,J-3)*TM(IIHE,J-3)+ZI(IIHE+1,J-1)*TM(IIHE+1,J-1)
    212      8       +ZI(IIHW,J-3)*TM(IIHW,J-3)+ZI(IIHE+1,J+1)*TM(IIHE+1,J+1) 

