    181 12349   format(' 5th row=',4(1x,f2.0))
    182 12350   format(' 6th row=',4(1x,f2.0))
    183 12351   format(' 7th row=',4(1x,f2.0))
    184 c     endif
    185       HTOT=TM(I,J)                              
    186      1    +TM(IIHW,J-1)+TM(IIHE,J-1)+TM(IIHE,J+1)+TM(IIHW,J+1)
    187      2    +TM(I-1,J-2)+TM(I+1,J-2)+TM(I+1,J+2)+TM(I-1,J+2)
