   1063 C-----------------------------------------------------------------
   1064                       SUBROUTINE INDTABLE 
   1065 C-----------------------------------------------------------------
   1066 C***
   1067 C***  THIS ROUTINE GENERATES THE TABLES THAT WILL GIVE THE
   1068 C***  STARTING AND ENDING VALUES OF I AND J FOR EACH PE ON
   1069 C***  THE GLOBAL AND LOCAL DOMAINS.  EACH PE WILL HAVE A COPY
   1070 C***  OF THE FULL TABLES.  THE ARGUMENT USED IS SIMPLY THE
   1071 C***  NUMBER OF THE PE FOR WHICH THESE VALUES ARE DESIRED.
   1072 C***
   1073 C-----------------------------------------------------------------
   1074       INCLUDE "parmeta"
   1097       INCLUDE "mpif.h"
   1659       INCLUDE "mpp.h"
   1660 C-----------------------------------------------------------------
   1661 C
   1720       IS_LOC_TABLE(MYPE)=MY_IS_LOC
   1721       JS_LOC_TABLE(MYPE)=MY_JS_LOC
   1722       IE_LOC_TABLE(MYPE)=MY_IE_LOC
   1723       JE_LOC_TABLE(MYPE)=MY_JE_LOC
   1724 C
   1725       IS_GLB_TABLE(MYPE)=MY_IS_GLB
   1726       IE_GLB_TABLE(MYPE)=MY_IE_GLB
   1727       JS_GLB_TABLE(MYPE)=MY_JS_GLB
   1728       JE_GLB_TABLE(MYPE)=MY_JE_GLB
   1729 C
   1730       DO IPE=0,NPES-1
   1731         CALL MPI_BCAST(IS_LOC_TABLE(IPE),1,MPI_INTEGER,IPE,
   1732      1                 MPI_COMM_COMP,IRTN)
   1733         CALL MPI_BCAST(JS_LOC_TABLE(IPE),1,MPI_INTEGER,IPE,
   1734      1                 MPI_COMM_COMP,IRTN)
   1735         CALL MPI_BCAST(IE_LOC_TABLE(IPE),1,MPI_INTEGER,IPE,
   1736      1                 MPI_COMM_COMP,IRTN)
   1737         CALL MPI_BCAST(JE_LOC_TABLE(IPE),1,MPI_INTEGER,IPE,
   1738      1                 MPI_COMM_COMP,IRTN)
   1739 C
   1740         CALL MPI_BCAST(IS_GLB_TABLE(IPE),1,MPI_INTEGER,IPE,
   1741      1                 MPI_COMM_COMP,IRTN)
   1742         CALL MPI_BCAST(JS_GLB_TABLE(IPE),1,MPI_INTEGER,IPE,
   1743      1                 MPI_COMM_COMP,IRTN)
   1744         CALL MPI_BCAST(IE_GLB_TABLE(IPE),1,MPI_INTEGER,IPE,
   1745      1                 MPI_COMM_COMP,IRTN)
   1746         CALL MPI_BCAST(JE_GLB_TABLE(IPE),1,MPI_INTEGER,IPE,
   1747      1                 MPI_COMM_COMP,IRTN)
   1748       ENDDO
   1749 C
   1750       CALL MPI_BARRIER(MPI_COMM_COMP,IRTN)
   1751 C------------------------------------------------------------------
   1752 C***
   1753 C***  ALL OF THE PEs CAN NOW GENERATE A COMPLETE TABLE OF THE
   1754 C***  NUMBER OF GRID POINTS IN THE I DIRECTION THAT ARE ON
   1755 C***  ALL OTHER PEs.  THIS WILL BE USED IN THE MESINGER MSLP
   1756 C***  REDUCTION AS WELL AS IN THE BROADCAST BELOW.
   1757 C***
   1758       DO IPE=0,NPES-1
   1759         ICHUNKTAB(IPE)=IE_LOC_TABLE(IPE)-IS_LOC_TABLE(IPE)+1

Page 21          Source Listing                  INDTABLE
2022-02-15 16:37                                 MPPINIT.F

   1760       ENDDO
   1761 C***
   1762 C***  SET UP A MAP OF THE GLOBAL DOMAIN THAT GIVES THE PE THAT
   1763 C***  OWNS EACH POINT.
   1764 C***  (THIS APPEARS TO BE VESTIGIAL)
   1765 C***
   1766 C
   1767 C***  FIRST EACH PE FILLS IN ITS SECTION OF THE ARRAY
   1768 C
   1769       DO JGLB=JS_GLB_TABLE(MYPE),JE_GLB_TABLE(MYPE)
   1770       DO IGLB=IS_GLB_TABLE(MYPE),IE_GLB_TABLE(MYPE)
   1771         ITEMP(IGLB,JGLB)=MYPE
   1772       ENDDO
   1773       ENDDO
   1774 C
   1775 C***  NEXT, ALL PEs EXCHANGE THEIR SECTIONS SO EVERYONE HAS
   1776 C***  A FULL MAP
   1777 C
   1778       DO IPE=0,NPES-1
   1779       DO JGLB=JS_GLB_TABLE(IPE),JE_GLB_TABLE(IPE)
   1780         CALL MPI_BCAST(ITEMP(IS_GLB_TABLE(IPE),JGLB),ICHUNKTAB(IPE)
   1781      1,                MPI_INTEGER,IPE,MPI_COMM_COMP,IRECV)
   1782       ENDDO
   1783       ENDDO
   1784 C
   1785       CALL MPI_BARRIER(MPI_COMM_COMP,IRTN)
   1786 C********************************************************************
   1787        RETURN
   1788        END

