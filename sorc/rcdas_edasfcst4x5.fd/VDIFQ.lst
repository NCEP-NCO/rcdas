     81 C-----------------------------------------------------------------------
     82       Q2(LMHM)=(DTOZS*AKQS*Q2(LMHK)-RSQ2(LMH2)*CF+Q2(LMHM))
     83      &        /((AKQ(LMH2)+AKQS)*DTOZS-CR(LMH2)*CF+1.)
     84 C
     85           DO 120 IVI=1,LMH2
     86       L=LMHM-IVI
     87       Q2(L)=(-CR(L)*Q2(L+1)+RSQ2(L))/CM(L)
     88  120  CONTINUE
     89 C-----------------------------------------------------------------------
     90  300                         CONTINUE
     91 C-----------------------------------------------------------------------
     92                              RETURN
     93                              END


