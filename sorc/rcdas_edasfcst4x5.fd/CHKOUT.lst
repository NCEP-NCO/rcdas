   2248       SUBROUTINE COAL(A,LEN)
   2249       INCLUDE "BUFFER.comm"
   2258       INCLUDE 'mpif.h'
   2820       REAL A(*)
   2821       IF ( LEN .LT. 0 ) THEN
   2822          IP = 0
   2823       END IF
   2824       IF ( IP + LEN .GT. IBUFMAX ) THEN
   2825          PRINT *, ' IBUFMAX in BUFFER.comm is too small, stopping'
   2826          PRINT *, ' CHANGE IBUFMAX in parmbuf and recompile'       
   2827          PRINT *, ' IBUFMAX=', IBUFMAX, ' WANT AT LEAST ', IP + LEN
   2828          CALL MPI_ABORT(MPI_COMM_WORLD,1,IERR)
   2829       ENDIF
   2830       DO I = 1, ABS(LEN)
   2831          IP = IP + 1
   2832          BUF(IP) = A(I)
   2833       ENDDO
   2834       return
   2835       END


