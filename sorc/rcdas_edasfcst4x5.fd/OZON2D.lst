    204       ENDDO
    205    40 CONTINUE
    206 C
    207       DO 50 I=MYIS,MYIE
    208       IF(POZN(I,K).LT.PRGFDL(1))THEN
    209         QO3(I,K)=QO3O3(I,1)
    210       ELSE IF(POZN(I,K).GT.PRGFDL(NL))THEN
    211         QO3(I,K)=QO3O3(I,NL)
    212       ELSE
    213         APLO=ALOG(PRGFDL(JJROW(I)-1))
    214         APHI=ALOG(PRGFDL(JJROW(I)))
    215         QO3(I,K)=QO3O3(I,JJROW(I))+(ALOG(POZN(I,K))-APHI)/
    216      1             (APLO-APHI)*
    217      2             (QO3O3(I,JJROW(I)-1)-QO3O3(I,JJROW(I)))
    218       ENDIF
    219    50 CONTINUE
    220 C
    221    60 CONTINUE
    222 C----------------------------------------------------------------------
    223       RETURN
    224       END


