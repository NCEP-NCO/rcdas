    635 C
    694       integer ierr
    695        LNIP = IM/INPES
    696        LNJP=JM/JNPES
    697 c
    698 C***
    699 C***  NUM_PROCS IS THE NUMBER OF TASKS DOING THE QUILTING
    700 C***  IN THIS SERVER GROUP
    701 C***
    702       call mpi_comm_size(mpi_comm_comp,num_procs, ierr )
    703       call mpi_comm_rank(mpi_comm_comp,me, ierr )
    704 c
    705 c     if ( me .eq. 0 ) then
    706          print *, ' num_procs = ',num_procs

