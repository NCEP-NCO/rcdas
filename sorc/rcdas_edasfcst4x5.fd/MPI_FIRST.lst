    707 c     end if
    708 
    709       if ( num_procs .gt. JNPES ) then
    710          print *, ' too many MPI tasks, max is ',JNPES, ', stopping'
    711          call MPI_ABORT(MPI_COMM_WORLD,1,ierr)
    712       end if
    713 c
    714       if ( num_procs .gt. 1024 ) then
    715          print *, ' too many MPI tasks, max is 1024, stopping'
    716          call MPI_ABORT(MPI_COMM_WORLD,1,ierr)
    717       end if
    718 c
    719 C***
    720 C***  JS_X AND JS_Y ARE THE STARTING AND ENDING ROWS OF TASKS
    721 C***  IN THE MODEL FORECAST DECOMPOSITION THAT WILL BE SENDING
    722 C***  TO EACH QUILT TASK
    723 C***
    724 C***  JSTA IS THE FIRST FORECAST TASK AND JEND IS THE LAST
    725 C***  FORECAST TASK IN THE ENTIRE RANGE OF FORECAST TASKS
    726 C***  THAT WILL BE SENDING TO EACH QUILT TASK.  REMEMBER
    727 C***  THAT AN INTEGER NUMBER OF FORECAST TASK ROWS IS
    728 C***  SENT TO EACH QUILT TASK.
    729 C***
    730       do i = 0, num_procs - 1
    731          call para_range(0,JNPES-1,num_procs,i,js_x,je_x)
    732          jsta(i) = js_x * INPES
    733          jend(i) = jsta(i) + (je_x-js_x+1)*INPES -1
    734          if ( me .eq. 0 ) then
    735             print *, ' task id, jsta, end = ',i,jsta(i),jend(i)
    736          end if
    737       end do
    738 c
    739 c     locations
    740 C
    741 C***  PARAMETER LNIP (LNJP) IS SMALLEST THAT THE I (J) EXTENT
    742 C***  OF EACH SUBDOMAIN CAN BE.
    743 C***  IRMND (JRMND) IS THE NUMBER OF "REMAINDER" I (J) POINTS
    744 C***  THAT WILL BE GIVEN TO THE LEFTMOST (LOWERMOST) PEs.
    745 
    746       IRMND=MOD(IM,INPES)
    747       JRMND=MOD(JM,JNPES)
    748 
    749       DO IPE = 0, NPES - 1
    750 C
    751       IPOSN=MOD(IPE,INPES)+1
    752       JPOSN=IPE/INPES+1
    753 C
    754 C***  GLOBAL LIMITS OF THIS PE's SUBDOMAIN
    755 C
    756       MY_IS_GLB_A(IPE)=(IPOSN-1)*LNIP+MIN(IRMND,IPOSN-1)+1
    757       MY_IE_GLB_A(IPE)=MY_IS_GLB_A(IPE)+LNIP-1
    758       IF(IPOSN.LE.IRMND)MY_IE_GLB_A(IPE)=MY_IE_GLB_A(IPE)+1
    759 C
    760       MY_JS_GLB_A(IPE)=(JPOSN-1)*LNJP+MIN(JRMND,JPOSN-1)+1
    761       MY_JE_GLB_A(IPE)=MY_JS_GLB_A(IPE)+LNJP-1
    762       IF(JPOSN.LE.JRMND)MY_JE_GLB_A(IPE)=MY_JE_GLB_A(IPE)+1
    763 c

