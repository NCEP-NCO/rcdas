    764       if ( me .eq. 0 ) then
    765 c        print *, ' ipe,  MY_IS_GLB,MY_IE_GLB,MY_JS_GLB, MY_JE_GLB =',
    766 c    * ipe, MY_IS_GLB_A(IPE),MY_IE_GLB_A(IPE),
    767 c    * MY_JS_GLB_A(IPE),MY_JE_GLB_A(IPE)
    768       end if
    769       END DO
    770 c
    771 c     dimensioning information
    772 c
    773       MY_ISD = 1
    774       MY_IED = IM
    775       MY_JSD = MY_JS_GLB_A(jsta(me)) -2
    776       MY_JED = MY_JE_GLB_A(jend(me)) +2
    777       IF ( MY_JSD .lt. 1 ) MY_JSD = 1
    778       IF ( MY_JED .gt. JM ) MY_JED = JM
    779 C
    780       print *, ' ME, MY_ISD,MY_IED,MY_JSD,MY_JED = ',
    781      & me, MY_ISD,MY_IED,MY_JSD,MY_JED
    782 C
    783       jsta_i = MY_JS_GLB_A(jsta(me))
    784       jend_i = MY_JE_GLB_A(jend(me))
    785       jsta_im  = jsta_i
    786       jsta_im2 = jsta_i
    787       jend_im  = jend_i
    788       jend_im2 = jend_i
    789       if ( me .eq. 0 ) then
    790          jsta_im  = 2
    791          jsta_im2 = 3
    792       end if
    793       if ( me .eq. num_procs - 1 ) then
    794          jend_im  = jm - 1
    795          jend_im2 = jm - 2
    796       end if
    797 c
    798       print *, ' jsta_i,jend_i,jsta_im,jend_im,jsta_im2,jend_im2= ',
    799      * jsta_i,jend_i,jsta_im,jend_im,jsta_im2,jend_im2
    800 c     neighbors
    801 c
    802       iup = me + 1
    803       idn = me - 1
    804       if ( me .eq. 0 ) then
    805          idn = MPI_PROC_NULL
    806       end if
    807       if ( me .eq. num_procs - 1 ) then
    808          iup = MPI_PROC_NULL
    809       end if
    810 C
    811 c     print *, ' ME, NUM_PROCS = ',me,num_procs
