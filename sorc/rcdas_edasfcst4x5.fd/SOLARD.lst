    124 C***
    125       DATE = MOD(DAT,YEAR)
    126 C***
    127 C    SOLVE ORBIT EQUATIONS BY NEWTON'S METHOD
    128 C***
    129       EM=PI2*DATE/YEAR
    130       E=1.E0
    131       ITER = 0
    132  31   EP=E-(E-EC*SIN(E)-EM)/(1.E0-EC*COS(E))
    133       CR=ABS(E-EP)
    134       E=EP
    135       ITER = ITER + 1
    136       IF(ITER.GT.10) GOTO 1031
    137       IF(CR.GT.CCR) GO TO 31
    138  1031 CONTINUE
    139       R1=1.E0-EC*COS(E)
    140 C
    141       WRITE(6,1000)JYR,JMNTH,JDAY,JHR,R1
    142  1000 FORMAT('SUN-EARTH DISTANCE CALCULATION FINISHED IN SOLARD'/
    143      &       'YEAR=',I5,'  MONTH=',I3,'  DAY=',I3,' HOUR='
    144      &,      I3,' R1=',F9.4)
    145 C***
    146 C    RETURN TO RADTN
    147 C***
    148       RETURN
    149       END


