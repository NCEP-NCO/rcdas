    253       VKRMZ=(0.5*(Z(L)+Z(L+1))-Z(LMHP))*VKRM
    254       ELL(L)=     VKRMZ/(VKRMZ/EL0+1.)
    255  165      CONTINUE
    256           ENDIF
    257 C-----------------------------------------------------------------------
    258           DO 260 L=1,LMHM
    259       EL(L)=AMIN1(0.5*(ELL(L)+ELL(L+1)),ELM(L))
    260  260      CONTINUE
    261 C-----------------------------------------------------------------------
    262                              RETURN
    263                              END


