    107                              P A R A M E T E R
    108      &(LP1=LM+1,LM1=LM-1)
    109 C-----------------------------------------------------------------------
    110                              P A R A M E T E R
    111      &(EPSQ2=0.2,EPSL=0.32,EPSRU=1.E-12,EPSRS=1.E-12
    112      &,FH=1.01,ALPH=.20,VKRM=.40,ELFC=0.23    ,EL0MAX=1000.)
    113 C-----------------------------------------------------------------------
    114                              P A R A M E T E R
    115 C-----------------------------------------------------------------------
    116      &(G=9.8,BETA=1./270.,BTG=BETA*G
    117      &,PRT=1.0,GAM1=0.2222222222222222222
    118      &,A1=0.659888514560862645,A2=0.6574209922667784586
    119      &,B1=11.87799326209552761,B2=7.226971804046074028
    120      &,C1=0.000830955950095854396)
    121 C-----------------------------------------------------------------------
    122 CUN     &(G=9.8,BETA=1./270.,BTG=BETA*G
    123 CUN     &,PRT=1.0,GAM1=0.2222222222222222222
    124 CUN     &,A1=0.3310949523016403346,A2=0.8273378704055731278
    125 CUN     &,B1=5.959709141429526024,B2=3.626088092074591135
    126 CUN     &,C1=-0.3330651924968952113)
    127 C-----------------------------------------------------------------------
    128 CMY     &(G=9.8,BETA=1./270.,BTG=BETA*G
    129 CMY     &,PRT=0.8,GAM1=0.2222222222222222222
    130 CMY     &,A1=0.9222222350809054114,A2=0.7350190142719400952
    131 CMY     &,B1=16.60000023145629741,B2=10.10000014082581951
    132 CMY     &,C1=0.0805318118080613468)
    133 C--------------COEFFICIENTS OF THE TERMS IN THE DENOMINATOR-------------
    134                              P A R A M E T E R
    135      &(ADNM=18.*A1*A1*A2*(B2-3.*A2)*BTG
    136      &,ADNH= 9.*A1*A2*A2*(12.*A1+3.*B2)*BTG*BTG
    137      &,BDNM= 6.*A1*A1
    138      &,BDNH= 3.*A2*(7.*A1+B2)*BTG

