    445           GDFNB(I) = GDFNB(I) + DFN(I,LP1)*DFNTOP(I,N)
    446 330     CONTINUE
    447 350   CONTINUE
    448       DO 360 K=1,LP1
    449       DO 360 I=MYIS,MYIE
    450         FSWL(I,K) = UFSWL(I,K)-DFSWL(I,K)
    451 360   CONTINUE
    452       DO 370 K=1,L
