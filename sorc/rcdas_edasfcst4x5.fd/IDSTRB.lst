      3                      SUBROUTINE IDSTRB(ARRG,ARRL)
      4 C     ******************************************************************
      5 C$$$  SUBPROGRAM DOCUMENTATION BLOCK
      6 C                .      .    .
      7 C SUBPROGRAM:    IDSTRB      DISTRIBUTE INTEGER GLOBAL ARRAY TO LOCAL ONES
      8 C   PRGRMMR: BLACK           ORG: W/NP2      DATE: 97-08-29
      9 C
     10 C ABSTRACT:
     11 C     IDSTRB DISTRIBUTES THE ELEMENTS OF INTEGER GLOBAL ARRAY ARRG TO
     12 C     THE INTEGER LOCAL ARRAYS ARRL.  LG IS THE VERTICAL DIMENSION OF THE
     13 C     GLOBAL ARRAY.  LL IS THE VERTICAL DIMENSION OF THE LOCAL ARRAY.
     14 C     L1 IS THE SPECIFIC LEVEL OF ARRL THAT IS BEING FILLED DURING
     15 C     THIS CALL (PERTINENT WHEN LG=1 AND LL>1).
     16 C
     17 C PROGRAM HISTORY LOG:
     18 C   97-08-29  BLACK      - ORIGINATOR
     19 C   12-11-13  EBISUZAKI  - ARRX(:,:) -> ARRX(:)
     20 C
     21 C USAGE: CALL READ_NFCST FROM SUBROUTINE INIT
     22 C   INPUT ARGUMENT LIST:
     23 C     ARRG - GLOBAL ARRAY
     24 C
     25 C   OUTPUT ARGUMENT LIST:
     26 C     ARRL - LOCAL ARRAY
     27 C
     28 C   OUTPUT FILES:
     29 C     NONE
     30 C
     31 C   SUBPROGRAMS CALLED:
     32 C     UNIQUE: NONE
     33 C
     34 C     LIBRARY: NONE
     35 C
     36 C   COMMON BLOCKS: NONE
     37 C
     38 C   ATTRIBUTES:
     39 C     LANGUAGE: FORTRAN 90
     40 C     MACHINE : IBM SP
     41 C$$$
     42 C---------------------------------------------------------------------------
     43 C***  DISTRIBUTE ARRAYS FROM GLOBAL TO LOCAL
     44 C---------------------------------------------------------------------------
     45       INCLUDE "parmeta"
     69       INCLUDE "mpp.h"
    128       INCLUDE "mpif.h"
    129 # 1 "./sp.h" 1 
    130 # 4
    131 
    132 # 48 "IDSTRB.F" 2 
    133 C---------------------------------------------------------------------------
    695       INTEGER ARRG(IM,JM),ARRX(IM*JM)
    696      1,       ARRL(IDIM1:IDIM2,JDIM1:JDIM2)
    697       INTEGER ISTAT(MPI_STATUS_SIZE)
    698 C---------------------------------------------------------------------------
    699 C***

Page 2           Source Listing                  IDSTRB
2022-02-15 16:38                                 IDSTRB.F

    700 C***  PE0 FILLS ITS OWN LOCAL DOMAIN THEN PARCELS OUT ALL THE OTHER PIECES
    701 C***  TO THE OTHER PEs.
    702 C***
    703       IF(MYPE.EQ.0)THEN
    704 C
    705           DO JGLB=JS_GLB_TABLE(0),JE_GLB_TABLE(0)
    706             LOCJ=G2LJ(JGLB)
    707             DO IGLB=IS_GLB_TABLE(0),IE_GLB_TABLE(0)
    708               LOCI=G2LI(IGLB)
    709               ARRL(LOCI,LOCJ)=ARRG(IGLB,JGLB)
    710             ENDDO
    711           ENDDO
    712 C
    713         DO IPE=1,NPES-1
    714           KNT=0
    715 C
    716           DO JGLB=JS_GLB_TABLE(IPE),JE_GLB_TABLE(IPE)
    717           DO IGLB=IS_GLB_TABLE(IPE),IE_GLB_TABLE(IPE)
    718             KNT=KNT+1
    719             ARRX(KNT)=ARRG(IGLB,JGLB)
    720           ENDDO
    721           ENDDO
    722 C
    723           CALL MPI_SEND(ARRX,KNT,MPI_INTEGER,IPE,IPE
    724      1,                 MPI_COMM_COMP,ISEND)
    725         ENDDO
    726 C--------------------------------------------------------------------
    727 C***
    728 C***  ALL OTHER PEs RECEIVE THEIR PIECE FROM PE0 AND THEN FILL
    729 C***  THEIR LOCAL ARRAY.
    730 C***
    731       ELSE
    732         NUMVALS=(IE_GLB_TABLE(MYPE)-IS_GLB_TABLE(MYPE)+1)
    733      1         *(JE_GLB_TABLE(MYPE)-JS_GLB_TABLE(MYPE)+1)
    734         CALL MPI_RECV(ARRX,NUMVALS,MPI_INTEGER,0,MYPE
    735      1,               MPI_COMM_COMP,ISTAT,IRECV)
    736 C
    737         KNT=0
    738         DO J=MY_JS_LOC,MY_JE_LOC
    739         DO I=MY_IS_LOC,MY_IE_LOC
    740           KNT=KNT+1
    741           ARRL(I,J)=ARRX(KNT)
    742         ENDDO
    743         ENDDO
    744 C
    745       ENDIF
    746 C
    747       CALL MPI_BARRIER(MPI_COMM_COMP,IRTN)
    748 C
    749       RETURN
    750       END

