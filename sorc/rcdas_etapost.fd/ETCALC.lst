     58 C ELSE IT IT A SUM OF CANOPY EVAP, DIRECT SOIL EVAP AND TRANSPIRATION
     59 C ----------------------------------------------------------------------
     60       IF (ETP .GT. 0.) THEN      
     61         IF (ESD .GT. 0.) THEN
     62           ESNOW = ETA
     63         ELSE
     64 
     65 C ----------------------------------------------------------------------
     66 C CANOPY EVAPORATION
     67 C ----------------------------------------------------------------------
     68           IF (CMC .GT. 0) THEN
     69             IF (CMC .GT. CMCMAX) CMC = CMCMAX
     70             EC = VEGFAC*((CMC/CMCMAX)**CFACTR)*ETP
     71           ENDIF
     72 
     73 C ----------------------------------------------------------------------
     74 C DIRECT SOIL EVAPORATION A FUNCTION OF RELATIVE SOIL MOISTURE
     75 C AVAILABILITY, LINEAR WHEN FXEXP=1.
     76 C ----------------------------------------------------------------------
     77           SRATIO = (SMC-SMCDRY)/(SMCMAX-SMCDRY)
     78           IF (SRATIO .GT. 0.) THEN
     79             FX = SRATIO**FXEXP
     80             FX = MAX(0.,MIN(FX,1.))
     81           ELSE
     82             FX = 0.
     83           ENDIF
     84           EDIR = FX*(1.0-VEGFAC)*ETP
     85 
     86 C ----------------------------------------------------------------------
     87 C CALCULATE TRANSPIRATION AS A RESIDUAL OF THE TOTAL MINUS EDIR AND EC
     88 C ----------------------------------------------------------------------
     89           ETRANS = ETA - EDIR - EC
     90         ENDIF
     91         IF (ETRANS .LT. 0.) ETRANS = 0.
     92 
     93       ENDIF
     94 
     95       RETURN
     96       END

