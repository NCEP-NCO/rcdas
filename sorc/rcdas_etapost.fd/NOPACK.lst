    140 C     
    141 C
    142 C****************************************************************************
    143 C     START NOPACK HERE.
    144 C
    145 C     SET OUTPUT GRID TYPE.  WE ASSUME KGYTPE HOLDS THE ON84
    146 C     ID FOR THE OUTPUT GRID.
    147 C     UNIQUE ON84 LABELS FOR VARIOUS ETA DOMAINS BELOW:
    148 C              90 = STAGGERED 80KM E-GRID 
    149 C              91 = FILLED    80KM E-GRID
    150 C              92 = STAGGERED 40KM E-GRID
    151 C              93 = FILLED    40KM E-GRID
    152 C              94 = STAGGERED 30KM E-GRID
    153 C              95 = FILLED    30KM E-GRID
    154 C              96 = STAGGERED "BIG" 40KM E-GRID (EDAS)
    155 C     
    156       KGTYP = KGTYPE
    157 C     IF NEED BE, OPEN A UNIT TO THE OUTPUT FILE.  IF REQUESTED,
    158 C     WRITE DATE AND GRID INFO IF REQUESTED.
    159 C     
    160       IF (RITEHD) THEN
    161 C
    162 C        APPEND FORECAST HOUR TO SUFFIX FOR SEQUENTIAL BINARY FILE.
    163          IHR = NTSD/TSPH + 0.5
    164 C     
    165 C        GET FULL PATH FOR OUTPUT FILE FROM ENVIRONMENT VARIABLE
    166 C        COMSP WHICH IS SET IN THE SCRIPT RUNNING THE MODEL.
    167 C     
    168 C        CONSTRUCT FULL PATH-FILENAME FOR OUTPUT FILE
    169          ENVAR = ' '
    170          CALL get_environment_variable('COMSP',ENVAR)
    171          IF (ENVAR(1:6).EQ.BLANK) THEN
    172           WRITE(DESCR2,1000) IHR
    173  1000     FORMAT('.SbinF',I2.2)
    174           KDAT = INDEX(DATSET,' ') -1
    175           IF (KDAT.LE.0) KDAT = LEN(DATSET)
    176           FNAME = DATSET(1:KDAT) // DESCR2
    177          ELSE
    178           CALL get_environment_variable('tmmark',RESTHR)
    179           WRITE(DESCR2,1011) IHR
    180  1011     FORMAT(I2.2)
    181           KENV = INDEX(ENVAR,' ') -1
    182           IF (KENV.LE.0) KENV = LEN(ENVAR)
    183           KDAT = INDEX(DATSET,' ') -1

