    184           IF (KDAT.LE.0) KDAT = LEN(DATSET)
    185           FNAME = ENVAR(1:KENV) // DATSET(1:KDAT) // DESCR2(1:2)
    186      &              //'.'// RESTHR
    187          ENDIF
    188 C
    189 C        ASSIGN AND OPEN UNIT FOR DATA FILE.
    190          CLOSE(LUNOUT)
    191 C        CALL ASNUNIT(LUNOUT,'-F cos -N ibm',ier)
    192 C        IF (IER.NE.0) WRITE(STDOUT,*)
    193 C    X        'NOPACK:  ASNUNIT ERROR IER=',IER
    194          OPEN(LUNOUT,FILE=FNAME,FORM='UNFORMATTED')
    195          REWIND(LUNOUT)
    196 C
    197          RITEHD = .FALSE.
    198          WRITE(STDOUT,*)'NOPACK:  OPENED ',LUNOUT,' ',FNAME,
    199      X        ' ',DATSET
    200 C      
    201 C        IF REQUESTED, THE FIRST TWO "RECORDS" INCLUDE DATE 
    202 C        AND OUTPUT GRID INFORMATION.  WE WRITE THEM ONLY 
    203 C        ONCE TO EACH OUTPUT GRID.  
    204 C     
    205          IF (INDEX(DATSET,'NOHEAD').EQ.0) THEN
    206             IHH = NTSD/TSPH+D50
    207             IYY = IDAT(3)-1900
    208             IMM = IDAT(1)
    209             IDD = IDAT(2)
    210             WRITE(LUNOUT) IHRST,IMM,IDD,IYY,IHH
    211             WRITE(LUNOUT) KGTYP,PROJ,NORTH,IMOUT,JMOUT,
    212      X           POLEI,POLEJ,ALATVT,ALONVT,XMESHL
    213             WRITE(STDOUT,*)'NOPACK:  WROTE DATE AND GRID INFO'
    214             WRITE(STDOUT,1010) IHRST,IMM,IDD,IYY,IHH
    215             WRITE(STDOUT,1020) KGTYP,PROJ,NORTH,IMOUT,JMOUT
    216             WRITE(STDOUT,1030) POLEI,POLEJ,ALATVT,ALONVT
    217             WRITE(STDOUT,1030) XMESHL
    218  1010       FORMAT(5(I5,1X))
    219  1020       FORMAT(I5,1X,A6,1X,L1,1X,I5,1X,I5)
    220  1030       FORMAT(4(G12.6,1X))
