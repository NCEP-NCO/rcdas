    221          ENDIF
    222       ENDIF
    223 C     
    224 C     SET SURFACE VALUE FOR DATA.
    225 C
    226       SFC = 0.
    227       IF (IS(IDENT(IFLD)).EQ.8) THEN
    228          SFC = SPL(ILVL)
    229       ELSEIF (IS(IDENT(IFLD)).EQ.149) THEN
    230 C
    231 C   TKE IS ON THE ETA INTERFACE AT THE BOTTOM OF THE LAYER ILVL
    232 C
    233          SFC = AETA(ILVL)
    234          IF (IQ(IDENT(IFLD)).EQ.158) SFC = ETA(ILVL+1)
    235       ENDIF
    236 C
    237 C     IF DATSET CONTAINS "NOHEAD" WE SKIP THE HEADER STUFF 

