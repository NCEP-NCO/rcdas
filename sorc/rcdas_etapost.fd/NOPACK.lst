    238 C     THAT FOLLOWS AND GO DIRECTLY TO THE DATA.  A USER 
    239 C     WOULD USED THIS OPTION IF (S)HE WANTS TO DIRECTLY
    240 C     CONTOUR DATA USING NMCIDAS.
    241 C     
    242       HEADER = .FALSE.
    243       IF (INDEX(DATSET,'NOHEAD').NE.0) GOTO 10
    244       WRITE(LUNOUT) FIELD(IFLD),SFC
    245       HEADER = .TRUE.
    246 C     
    247 C     WRITE DATA TO OUTPUT FILE.
    248 C     
    249  10   CONTINUE
    250       WRITE(LUNOUT) GRID
    251       CALL MINMAX(GRID,IMOUT,JMOUT,FMIN,FMAX)
    252       WRITE(STDOUT,1040) IFLD,FIELD(IFLD),SFC,FMIN,FMAX,
    253      X     KGTYP,HEADER
