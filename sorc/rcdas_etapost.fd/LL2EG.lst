     63 C********************************************************************
     64 C     START LL2EG HERE.
     65 C     
     66 C     CONVERT (LAT,LON) TO FLOATING POINT (I,J).
     67       PHI    = YLAT*CONV2R
     68       LAM    = WLON*CONV2R
     69       COSPHI = COS(PHI)
     70       SINPHI = SIN(PHI)
     71 
     72       X1     = COSPHI*COS(LAM-LAM0)
     73       X      = COSPH0*X1+SINPH0*SINPHI
     74       Y      = -COSPHI*SIN(LAM-LAM0)
     75       Z      = -SINPH0*X1+COSPH0*SINPHI
     76       BIGPHI = ATAN(Z/(SQRT(X*X+Y*Y)))*CONV2D
     77       BIGLAM = ATAN(Y/X)*CONV2D
     78       XI     = (BIGLAM/DLAM)+(IDIM+1)/2.0
     79       YJ     = (BIGPHI/DPHI)+(JDIM+1)/2.0
     80 C     
     81 C     END OF ROUTINE.
     82 C
     83       RETURN
     84       END


