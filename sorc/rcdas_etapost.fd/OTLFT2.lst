    336 C
    337 C--------------BASE AND SCALING FACTOR FOR THE--------------------------
    338 C
    339       DO 220 J=JSTA_M2,JEND_M2
    340       DO 220 I=2,IM-1
    341       IPTBK=IPTB(I,J)
    342       BTHE00(I,J)=THE0(IPTBK)
    343       STHE00(I,J)=STHE(IPTBK)
    344       BTHE10(I,J)=THE0(IPTBK+1)
    345       STHE10(I,J)=STHE(IPTBK+1)
