    369         PP(I,J)=D00
    370       ENDIF
    371  235  CONTINUE
    372 C
    373 C--------------TEMPERATURE AT FOUR SURROUNDING TT TABLE PTS.------------
    374 C
    375       DO 240 J=JSTA_M2,JEND_M2
    376       DO 240 I=2,IM-1
    377       ITH=ITHTB(I,J)
    378       IP=IPTB(I,J)
    379       T00(I,J)=TTBL(ITH,IP)
    380       T10(I,J)=TTBL(ITH+1,IP)
    381       T01(I,J)=TTBL(ITH,IP+1)
    382       T11(I,J)=TTBL(ITH+1,IP+1)
    383   240 CONTINUE
    384 C
    385 C--------------PARCEL TEMPERATURE AT 500MB----------------------------
    386 C
    387       DO 300 J=JSTA_M2,JEND_M2
    388       DO 300 I=2,IM-1
    389       IF(TPSP(I,J).GE.H5E4)THEN
    390         PARTMP(I,J)=(T00(I,J)+(T10(I,J)-T00(I,J))*PP(I,J)
    391      1                       +(T01(I,J)-T00(I,J))*QQ(I,J)
    392      2         +(T00(I,J)-T10(I,J)-T01(I,J)+T11(I,J))*PP(I,J)*QQ(I,J))
    393       ELSE
    394         PARTMP(I,J)=TBT(I,J)*APEBT(I,J)*D8202
    395       ENDIF
    396 C
    397 C--------------LIFTED INDEX---------------------------------------------
    398 C
    399       SLINDX(I,J)=T500(I,J)-PARTMP(I,J)
    400   300 CONTINUE
