     44 C
     45       INCLUDE "CTLBLK.comm"
     46 C     
     47 C*******************************************************************
     48 C     START MINMAX HERE.
     49 C     
     50 C     INITIALIZE VARIABLES.
     51 C      AMAX = -1.E50
     52 C      AMIN =  1.E50
     53 C WNE 6/2012
     68       AMAX=A(1,JSTA)
     69       AMIN=A(1,JSTA)
     70 C     
     71 C     FIND MIN AND MAX VALUES IN ARRAY.

