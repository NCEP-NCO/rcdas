    439 C        TO PROCESS.
    440 C     
    441          CALL READCNTRL2(IEOF)
    442          WRITE(STDOUT,*)'ETAPOST:  RETURN FROM READCNTRL.  ',
    443      1        'IEOF=',IEOF
    444          IF (IEOF.NE.0) GOTO 20
    445 C     
    446 C        PROCESS SELECTED FIELDS.  FOR EACH SELECTED FIELD/LEVEL
    447 C        WE GO THROUGH THE FOLLOWING STEPS:
    448 C           (1) COMPUTE FIELD IF NEED BE
    449 C           (2) INTERPOLATE FROM E-GRID TO OUTPUT GRID.
    450 C           (3) WRITE FIELD TO OUTPUT FILE IN REQUESTED FORMAT.
    451 C
    452          CALL PROCESS
    453          WRITE(STDOUT,*)' '
    454          WRITE(STDOUT,*)'ETAPOST:  PREPARE TO PROCESS NEXT GRID'
    455 C     
    456 C     PROCESS NEXT GRID.
    457 C     
    458       GO TO 10
    459 C     
    460 C     ALL GRIDS PROCESSED.
    461 C     
    462  20   CONTINUE
    463       WRITE(STDOUT,*)' '
