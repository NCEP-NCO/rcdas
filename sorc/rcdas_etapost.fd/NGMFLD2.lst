    425 C
    426          IF (Z8498(I,J).GT.0) THEN
    427             RH8498(I,J) = RH8498(I,J)/Z8498(I,J)
    428          ELSE
    429             RH8498(I,J) = SPVAL
    430          ENDIF
    431 C
    432          IF (Z4796(I,J).GT.0) THEN
    433             RH4796(I,J) = RH4796(I,J)/Z4796(I,J)
    434          ELSE
    435             RH4796(I,J) = SPVAL
