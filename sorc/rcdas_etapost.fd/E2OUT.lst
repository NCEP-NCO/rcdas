    240               CALL EFILT(EGRID1)
    241          END IF
    242          IF(ITAG2.GT.0)THEN
    243            ISMTH = ISMSTG(IGET(ITAG2))
    244            IF(ISMTH.GT.0) THEN 
    245               CALL P2FILTF(ISMTH,HBM2,EGRID2)
    246            END IF
    247          ENDIF
    248       ELSE
    249          IF (ISMTH.GT.0) THEN
    250               CALL P2FLTVF(ISMTH,VBM2,EGRID1)
    251          END IF
    252          IF(ITAG2.GT.0)THEN
    253            ISMTH = ISMSTG(IGET(ITAG2))
    254            IF(ISMTH.GT.0) THEN
    255               CALL P2FLTVF(ISMTH,VBM2,EGRID2)
    256            END IF

