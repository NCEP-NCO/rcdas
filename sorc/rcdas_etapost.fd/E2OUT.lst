    261 C     
    262       IF ( IFILV(ITAG1).GT.0) THEN
    263 C     
    264 C        FILL H-POINT FIELD AT V POINTS.
    265 c         call psd
    266          CALL FILLV(EGRID1,HFUL,IFLAG,IMT,JMT)
    267 c          call pse
