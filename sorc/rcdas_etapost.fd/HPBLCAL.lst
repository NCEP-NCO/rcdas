      2       SUBROUTINE HPBLCAL(I,J,LM,LMHK,LPBL,HPBL,Q2,Z)
      3 C **********************************************************************
      4 C *      PBL HEIGHT (HPBL) CALCULATION BASED ON LEVEL 2.5 MIXING       *
      5 C   03-01-23 - M EK LIFTED HPBL COMPUTATION FROM ETA MODEL
      6 C **********************************************************************
      7 C-----------------------------------------------------------------------
      8                              P A R A M E T E R
      9      &(EPSQ2=0.2,FH=1.01)
     10       
     11       REAL Q2(LM),Z(LM+1)
     12       LMHP=LMHK+1
     13 C--------------FIND THE HEIGHT OF THE PBL-------------------------------
     14       LPBL=LMHK
     15           DO 100 IVI=1,LMHK
     16       L=LMHK-IVI
     17       IF(Q2(L).LE.EPSQ2*FH)THEN
     18 CVVVVVVVVVVVVVV NOT NECESSARY IF DRIVEN BY TURBL VVVVVVVVVVVVVVVVVVVVVVV
     19 C        Q2(L)=EPSQ2
     20 CAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA
     21         LPBL=L
     22         GO TO 110
     23       ENDIF
     24  100  CONTINUE
     25       LPBL=1
     26 C--------------THE DEPTH OF THE PBL------------------------------------
     27  110  HPBL=Z(LPBL)-Z(LMHP)
     28       if(i.eq.550.and.j.eq.144)print*
     29      +, 'Debug:sample HPBLCAL in HPBLCAL'
     30      +,I,J,LM,LMHK,LPBL,HPBL,(Q2(L),l=1,lm)
     31      +,(Z(L),l=1,lm+1) 
     32 C-----------------------------------------------------------------------
     33       RETURN
     34       END

