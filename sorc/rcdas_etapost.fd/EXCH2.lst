    297       end if
    298       call mpi_sendrecv(a(1,jsta  ),2*im,MPI_REAL,idn,1,
    299      &                  a(1,jend+1),2*im,MPI_REAL,iup,1,
    300      &                  MPI_COMM_WORLD,status,ierr)
    301       if ( ierr .ne. 0 ) then
    302          print *, ' problem with second sendrecv in exch2, ierr = ',ierr
    303          stop
    304       end if
    305 c

