     72       DO I=1,IM
     73         FLD(I,J) = AMAX1(FMIN,FLD(I,J))
     74         FLD(I,J) = AMIN1(FLD(I,J),FMAX)
     75       ENDDO
     76       ENDDO
     77 C     
     78 C     END OF ROUTINE.
     79 C     
     80       RETURN
     81       END


