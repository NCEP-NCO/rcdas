    151       DO I=1,IM-1
    152         FNE(I,J)=WFIELD(I+IHE(J),J+1)-WFIELD(I,J)
    153       ENDDO
    154       ENDDO
    155 C
    156 !$omp  parallel do
    157       DO J=JSTA_M,JEND
    158       DO I=1,IM-1
    159         FSE(I,J)=WFIELD(I+IHE(J),J-1)-WFIELD(I,J)
    160       ENDDO
    161       ENDDO
    162 C
    163       CALL EXCH(FNE)
    164       CALL EXCH(FSE)
    165 C
    166 !$omp  parallel do
    167       DO J=JSTA_M2,JEND_M2
    168       DO I=2,IM-1
    169         FIELD(I,J)=FIELD(I,J)-(FNE(I,J)-FNE(I+IHW(J),J-1)
    170      1                        +FSE(I,J)-FSE(I+IHW(J),J+1))
    171      2                        *HBM2(I,J)*0.125
    172       ENDDO
    173       ENDDO
    174 C
    175  400  CONTINUE
    176 C
    177       CALL EXCH(FIELD)
    178 C     
    179 C     END OF ROUTINE
    180       RETURN
    181       END

