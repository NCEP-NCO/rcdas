    137         FSE(I,J)=WFIELD(I+IVE(J),J-1)-WFIELD(I,J)
    138       ENDDO
    139       ENDDO
    140 C
    141 !$omp  parallel do
    142       DO J=3,JM-2
    143       DO I=2,IM-1
    144         FIELD(I,J)=FIELD(I,J)-(FNE(I,J)-FNE(I+IVW(J),J-1)
    145      1                        +FSE(I,J)-FSE(I+IVW(J),J+1))
    146      2                        *VBM2(I,J)*0.125
    147       ENDDO
    148       ENDDO
    149 C
    150  400  CONTINUE
    151 C     
    152 C     END OF ROUTINE
    153       RETURN
    154       END


