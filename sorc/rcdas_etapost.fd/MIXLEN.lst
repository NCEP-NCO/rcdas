     58 C***********************************************************************
     59 C
     74 !$omp  parallel do
     75       DO L=1,LM
     76         DO J=JSTA,JEND
     77         DO I=1,IM
     78           EL(I,J,L)=0.
     79         ENDDO
     80         ENDDO
     81       ENDDO
     82 C
     83 C---THE AVERAGE EL SCHEME---------------------------(FM, AUGUST 19 MEMO)
     84 C   FIRST GET EL IN THE LAYERS
     85 C
     86 !$omp  parallel do
     87 !$omp& private(vkrmz,zl)
     88       DO 260 L=1,LM
     89       DO J=JSTA_M,JEND_M
     90       DO I=2,IM-1
     91         ZL=0.5*(ZINT(I,J,L)+ZINT(I,J,L+1))
     92         VKRMZ=(ZL-HGT(I,J))*VKRM
     93         EL(I,J,L)=EL0(I,J)*VKRMZ/(EL0(I,J)+VKRMZ)
     94       ENDDO
     95       ENDDO
     96   260 CONTINUE
     97 C***
     98 C***  GET NOW THE INTERFACE EL BY TWO-POINT AVERAGING OF LAYER VALUES
     99 C***
    100       DO 280 L=1,LM1
    101 !$omp  parallel do
    102       DO J=JSTA_M,JEND_M
    103       DO I=2,IM-1
    104         EL(I,J,L)=0.5*(EL(I,J,L)+EL(I,J,L+1))*HTM(I,J,L+1)
    105       ENDDO
    106       ENDDO
    107  280  CONTINUE
    108 C
    109 !$omp  parallel do
    110       DO J=JSTA,JEND
    111       DO I=1,IM
    112         EL(I,J,LM)=0.0
    113       ENDDO
    114       ENDDO
    115 C---STABILITY, PBL TOP, AND VERTICAL GRID DISTANCE RESTRICTIONS:--------
    116 C   COMPUTE EL STABLE AND
    117 C   * USE THE SMALLER OF EL BLACKADAR, EL STABLE IF WITHIN PBL;
    118 C   * USE THE SMALLEST OF EL STABLE, ELVGD, AND VKRMZ IF ABOVE PBL
    119 C       (ASSUME PBL TOP IS AT CPBLT*EL0(K));
    120       DO J=JSTA_M,JEND_M
    121       DO I=2,IM-1
    122         APE(I,J,1)=(1.E5/(PDSL(I,J)*AETA(1)+PT))**CAPA
    123       ENDDO
    124       ENDDO
    125 C
    126       DO 380 L=1,LM1
    127 !$omp  parallel do
    128 !$omp& private(elst,elvgd,ensq,q2kl,ziag)

